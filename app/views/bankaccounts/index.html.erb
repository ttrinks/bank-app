<p id="notice"><%= notice %></p>

<h1>Bankaccounts</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Account type</th>
      <th>Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bankaccounts.each do |bankaccount| %>
      <tr>
        <td><%= bankaccount.client_id %></td>
        <td><%= bankaccount.account_type %></td>
        <td><%= bankaccount.balance %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
